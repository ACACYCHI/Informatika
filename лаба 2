#include <iostream>

int main()
{
	int n=10;
	int a[n];
	for (int i=0;i < n;i++){
		std::cin >> a[i];
	}
	int max=a[0];
	int i;
	for (int i=1;i < n;i++){
		if (a[i] > max){
			max = a[i];
		}
			
	}
	std::cout << "max=" << max << "   ";
	
	int Sum=0;
	for (int i=0;i < n;i++){
		Sum = a[i] + Sum;
	}
	std::cout << "sum=" << Sum << std::endl;
	
	std::cout << "a" << std::endl;
	str::cin >> int a;
	std::cout << "b" << std::endl;
	str::cin >> int b;
	(не доделал)
}
